{
    "name": "resource-handler",
    "version": "1.1.0",
    "description": "Thin wrapper around async resource",
    "main": "lib/index.js",
    "scripts": {
        "gh-pages": "git subtree push --prefix docs origin gh-pages",
        "doc": "rimraf docs && typedoc --excludeProtected --excludePrivate --out docs && touch docs/.nojekyll",
        "fmt": "pretty-quick",
        "clean": "rimraf lib",
        "lint": "eslint src --ext .ts",
        "test": "cross-env NODE_ENV=test mocha",
        "build": "cross-env NODE_ENV=production npm run lint && npm run clean && tsc -b tsconfig.json",
        "prepublish": "npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ziflex/resource-handler.git"
    },
    "author": "Tim Voronov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ziflex/resource-handler/issues"
    },
    "keywords": [
        "async",
        "await",
        "connection",
        "wrapper",
        "handler",
        "auto",
        "reconnect"
    ],
    "homepage": "https://github.com/ziflex/resource-handler#readme",
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/mocha": "^8.0.2",
        "@types/node": "^14.0.27",
        "@types/sinon": "^9.0.4",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "chai": "^4.2.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.2.5",
        "mocha": "^8.1.1",
        "mocha-lcov-reporter": "^1.3.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "rimraf": "^3.0.2",
        "sinon": "^9.0.3",
        "ts-node": "^8.10.2",
        "typedoc": "^0.18.0",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "p-retry": "^4.2.0"
    }
}
